@model List<DoctorAppointmentBookingSystem.Models.RegisterModel>
@{
    ViewBag.Title = "Index";
    int row = 0;

}

        <h5 class="card-title fw-semibold mb-4">Register List</h5>
        <a href="/Register/Create" type="button" class="btn btn-primary mb-3">Create</a>
        <table id="myTable" class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>No</th>
                    <th>UserName</th>
                    <th>LoginName</th>
                    <th class="d-none">Password</th>
                    <th>RoleName</th>
                    <th></th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    row = row + 1;
                    <tr>
                        <td>@row</td>
                        <td>@item.LoginName</td>
                        <td>@item.UserName</td>
                        <td class="d-none">@item.Password</td>
                        <td>@item.RoleName</td>
                        <td>

                            <a href="/Register/Edit?id=@item.Id" type="button" class="btn btn-success">Edit</a>

                        </td>
                        <td>
                            <a href="/Register/Delete?id=@item.Id" type="button" class="btn btn-danger">Delete</a>
                        </td>
                    </tr>
                }

            </tbody>
        </table>




